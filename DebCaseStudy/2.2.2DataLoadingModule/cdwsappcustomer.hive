create external table if not exists CDW_SAPP_HIVEDB.CUSTOMER (CUST_SSN INT, CUST_F_NAME STRING, CUST_M_NAME STRING, CUST_L_NAME STRING, CUST_CC_NO STRING, CUST_STREET STRING, CUST_CITY STRING, CUST_STATE STRING, CUST_COUNTRY STRING, CUST_ZIP INT, CUST_PHONE STRING, CUST_EMAIL STRING, LAST_UPDATED TIMESTAMP) ROW FORMAT DELIMITED FIELDS TERMINATED BY ',' LINES TERMINATED BY '\n' LOCATION '/Credit_Card_System/CDW_SAPP_CUSTOMER';       
SET hive.exec.dynamic.partition=true;
SET hive.exec.dynamic.partition.mode=nonstrict;   

CREATE TABLE IF NOT EXISTS CDW_SAPP_HIVEDB.EXTERNALCUSTOMER (CUST_SSN INT, CUST_F_NAME STRING, CUST_M_NAME STRING, CUST_L_NAME STRING, CUST_CC_NO STRING, CUST_STREET STRING, CUST_CITY STRING, CUST_STATE STRING, CUST_COUNTRY STRING, CUST_ZIP INT, CUST_PHONE STRING, CUST_EMAIL STRING, LAST_UPDATED TIMESTAMP) ROW FORMAT DELIMITED FIELDS TERMINATED BY ',' LINES TERMINATED BY '\n' LOCATION '/Credit_Card_System/CDW_SAPP_EXTERNALCUSTOMER'; 

INSERT OVERWRITE TABLE CDW_SAPP_HIVEDB.EXTERNALCUSTOMER PARTITION (CUST_COUNTRY) SELECT CUST_SSN, CUST_F_NAME, CUST_M_NAME, CUST_L_NAME, CUST_CC_NO, CUST_STREET, CUST_CITY, CUST_STATE, CUST_ZIP, CUST_PHONE, CUST_EMAIL, LAST_UPDATED, CUST_COUNTRY FROM CDW_SAPP_HIVEDB.CUSTOMER;